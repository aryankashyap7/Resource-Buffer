#include <bits/stdc++.h>
using namespace std;

int main()
{
    int cases, a, b;
    cin >> cases;
    for (int i = 0; i < cases; i++)
    {
        cin >> a >> b;
        if ((a == 2 & b == 1) || (a == 1 & b == 2) || (a == 0 & b == 0))
            cout << "YES" << endl;
        else if (a % 3 == 0 && b % 3 == 0)
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
    return 0;
}
